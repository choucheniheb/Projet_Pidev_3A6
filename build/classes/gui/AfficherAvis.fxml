<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.AfficherAvisController">
   <children>
      <ImageView fitHeight="400.0" fitWidth="600.0">
         <image>
            <Image url="@images/ELJEM.jpg" />
         </image>
      </ImageView>
      <TableView fx:id="AvisTv" layoutX="20.0" layoutY="95.0" prefHeight="256.0" prefWidth="561.0">
        <columns>
          <TableColumn fx:id="id_avisTv" prefWidth="75.0" text="id_avis" />
          <TableColumn fx:id="id_utilisateurTv" prefWidth="75.0" text="id_utilisateur" />
            <TableColumn fx:id="text_avisTv" prefWidth="75.0" text="text_avis" />
            <TableColumn fx:id="date_avisTv" prefWidth="75.0" text="date_avis" />
            <TableColumn fx:id="rate_avisTv" prefWidth="103.0" text="rate_avis" />
            <TableColumn fx:id="delete" minWidth="1.0" prefWidth="73.0" text="action" />
            <TableColumn fx:id="modifier" minWidth="1.0" prefWidth="93.0" text="action1" />
        </columns>
      </TableView>
      <Label layoutX="179.0" layoutY="6.0" prefHeight="42.0" prefWidth="262.0" text="Affichage Des Avis" textFill="#eeeded">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <Label fx:id="welcomeLb" layoutX="208.0" layoutY="59.0" prefHeight="17.0" prefWidth="233.0" text="welcome back" textFill="#ebe1e1">
         <font>
            <Font name="System Italic" size="25.0" />
         </font>
      </Label>
      <Button fx:id="exportExcelBtn" layoutX="501.0" layoutY="362.0" mnemonicParsing="false" onMouseClicked="#handleImageViewClick" text="export " />
   </children>
</AnchorPane>
